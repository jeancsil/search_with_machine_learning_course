{
  "featureset": {
    "features": [
      {
        "name": "name_match",
        "params": [
          "keywords"
        ],
        "template_language": "mustache",
        "template": {
          "match": {
            "name": "{{keywords}}"
          }
        }
      },
      {
        "name": "sku_match",
        "params": [
          "skus"
        ],
        "template_language": "mustache",
        "template": {
          "query_string": {
            "query": "{{skus}}",
            "fields": [
              "_id"
            ]
          }
        }
      }
    ]
  }
}